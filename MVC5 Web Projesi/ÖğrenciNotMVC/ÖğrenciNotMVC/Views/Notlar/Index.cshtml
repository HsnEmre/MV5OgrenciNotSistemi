@using ÖğrenciNotMVC.Models.EntityFramework
@model List<Tbl_Notlar>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>
<table class="table table-bordered">
    <tr>
        <th>NOT ID</th>
        <th>ÖĞRENCİ ID</th>
        <th>DERS ID</th>
        <th>SINAV 1</th>
        <th>SINAV 2</th>
        <th>SINAV 3</th>
        <th>PROJE</th>
        <th>ORTALAMA</th>
        <th>DURUM</th>
    </tr>
    <tbody>
        @foreach (var not in Model)
        {
            <tr>
                <td>@not.NOTID</td>
                <td>@not.OGRID</td>
                <td>@not.DERSID</td>
                <td>@not.SINAV1</td>
                <td>@not.SINAV2</td>
                <td>@not.SINAV3</td>
                <td>@not.PORJE</td>
                <td>@not.ORTALAMA</td>
                <td>@not.DURUM</td>
            </tr>
        }
    </tbody>
</table>
